{
  "version": "1.0.0",
  "private": false,
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "e2e": "yarn cypress open",
    "e2e:run": "yarn cypress run",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "size:fresh": "rm -rf dist && yarn build && yarn size",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "diagnostics": false
      }
    }
  },
  "name": "@benjamminj/use-focus-trap",
  "author": "Ben Johnson",
  "module": "dist/use-focus-trap.esm.js",
  "size-limit": [
    {
      "path": "dist/use-focus-trap.cjs.production.min.js",
      "limit": "1 KB"
    },
    {
      "path": "dist/use-focus-trap.esm.js",
      "limit": "1 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.7.0",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "cypress": "^5.6.0",
    "cypress-plugin-tab": "^1.0.5",
    "husky": "^4.3.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "size-limit": "^4.7.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.0.3",
    "typescript": "^4.0.5"
  },
  "dependencies": {}
}
